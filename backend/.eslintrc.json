{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "import/prefer-default-export": 1,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "tabWidth": 2
      }
    ]
  }  
}